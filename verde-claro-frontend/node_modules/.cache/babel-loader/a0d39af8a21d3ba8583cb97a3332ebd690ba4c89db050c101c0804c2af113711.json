{\r\n      this.id = 0,\r\n        this.id = '',\r\n        this.nombre = '',\r\n        this.descripcion = '',\r\n        this.descuento = '',\r\n        this.precioUnitarioCompra = '',\r\n        this.listData = []\r\n    },\r\n    \r\n  }\r\n}\r\n\r\n\r\n</script>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}