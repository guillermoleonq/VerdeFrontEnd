ctos').then(result => {\r\n        this.listData = result.data\r\n      })\r\n    },\r\n    dataAdd(){\r\n      let data = {\r\n                id: this.id,\r\n                nombre: this.nombre,\r\n                descripcion: this.descripcion,\r\n                codigo: this.codigo,\r\n                codigo: this.codigo,\r\n            };\r\n            console.log(data);\r\n    },\r\n    clearList: function () {\r\n      this.id = 0,\r\n        this.id = '',\r\n        this.nombre = '',\r\n        this.descripcion = '',\r\n        this.descuento = '',\r\n        this.precioUnitarioCompra = '',\r\n        this.listData = []\r\n    },\r\n    \r\n  }\r\n}\r\n\r\n\r\n</script>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}